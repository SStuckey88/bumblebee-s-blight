{
	"name": "BumbleBee's Blight Color theme",
	"colors": {
		"editor.background": "#030100",
		"editor.foreground": "#ffffff",
		

		"contrastActiveBorder": "#e2a92b",
		//"contrastBorder": "#1EC4F26f",
		"contrastBorder": "#fcb43a67",


		"foreground": "#eddfc4",
		"descriptionForeground": "#dbbf2c",
		"icon.foreground": "#dddefc",

		//"window.activeBorder":  "#dbbf2c"  --not supported in windows 

		"list.focusBackground":"#14b1b1ff",
		"list.activeSelectionBackground": "#14b1b1ff",
		"list.activeSelectionForeground": "#ffd900",

		//"activityBar.background": "#afcbff",
		"activityBar.background": "#0b0a0a",
		"activityBar.foreground": "#ffe66c",
		"activityBar.border": "#fcb43aff",

		"titleBar.activeForeground": "#eddfc4",
		"titleBar.border": "#fcb43aff",
		"menubar.selectionBorder": "#00a6ffba",

		"terminal.border": "#ff0000",

		"button.background": "#eddfc4",
		"button.foreground": "#000040",
		"button.border": "#00a6ffba",
		
		"sideBar.background" : "#17161c",
		//"sideBar.foreground" : "#b8dbd2",
		"sideBar.foreground" : "#afcbff",
		"sideBar.border": "#fcb43aff",
		"sideBarSectionHeader.border": "#fcb43aff",

		"editorLineNumber.foreground": "#eddfc4",
		"editorLineNumber.activeForeground": "#ffe66c",

	},
	"tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#90eaf8"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#f69cd6"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#15cea8"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#b46695"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"scope": [
				"source.css entity.other.attribute-name",
				"source.css.less entity.other.attribute-name.id",
				"source.scss entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#282d31"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#3d3431"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#222420"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#394f5b"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#008cff"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#96eaa4"
			}
		},
		{
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#ff9cd6"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#deb3e5"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#ff2128"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#ff6969"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js"
			],
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less"
			],
			"settings": {
				"foreground": "#ff0444"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#b8dbd2"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#f69cd6"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#f98e02"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#eea7d5"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"name": "coloring of the TS this",
			"scope": "variable.language.this",
			"settings": {
				"foreground": "#f07706"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars"
			],
			"settings": {
				"foreground": "#DCDCAA"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.class",
				"source.cs storage.type"
			],
			"settings": {
				"foreground": "#52cea5"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#fb9781"
			}
		},
		{
			"name": "Control flow keywords",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#9de56f"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#e1ce59"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#9CDCFE"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#CE9178"
			}
		}
	]
}
